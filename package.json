{
  "name": "yr-media-nuxt-template",
  "version": "1.0.0",
  "description": "My stunning Nuxt.js project",
  "author": "Radames Ajna",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && rm -rf _site/* && cp -R dist/ _site/",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "gh-pages": "npm run generate && cd _site && git add . && git commit --amend --no-edit && git push  origin gh-pages --force"
  },
  "dependencies": {
    "@nuxtjs/google-gtag": "^1.0.4",
    "d3-array": "^2.4.0",
    "d3-composite-projections": "^1.3.2",
    "d3-dsv": "^1.2.0",
    "d3-ease": "^1.0.6",
    "d3-geo": "^1.12.0",
    "d3-selection": "^1.4.1",
    "d3-svg-annotation": "^2.5.1",
    "d3-transition": "^1.3.2",
    "gootenberg": "git://github.com/radames/gootenberg.git#test-security-updates",
    "howler": "^2.1.3",
    "lazysizes": "^5.2.0",
    "marked": "^1.0.0",
    "nuxt": "^2.12.2",
    "scrollmagic": "^2.0.7",
    "topojson-client": "^3.1.0",
    "vue-observe-visibility": "^0.4.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.2.0",
